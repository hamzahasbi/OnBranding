{
    "extends": "airbnb-base",
    "parserOptions": {
      "sourceType": "script",
      "ecmaVersion": 2020

    },
    "rules": {
      "strict": 0,
      "no-empty-function": "warn",
      "no-param-reassign": [
        "error",
        { "props": false }
      ],
      "quotes": ["warn", "single"],
      "indent": 0,
      "func-names": 0,
      "comma-dangle": 0,
      "prefer-template": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-unused-expressions": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "no-bitwise": ["error", { "allow": ["~"] }]
    },
    "globals": {
      "describe": true,
      "it": true,
      "after": true,
      "afterEach": true,
      "before": true,
      "beforeEach": true,
      "should": true
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    }
  }